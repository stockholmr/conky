${voffset 2}${font StyleBats:size=10}${color1}A${voffset -1}${font}${color}${offset 5}${execi 1000 cat /proc/cpuinfo | grep 'model name' | sed -e 's/model name.*: //'| uniq | cut -c 1-26}${offset 3}(${offset 2}${execpi 2 sensors | grep 'Package id 0:' | cut -c 17-18}°${offset 1})${alignr 1}${font}${freq_g cpu0}${offset 1}GHz${font}
